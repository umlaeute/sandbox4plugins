<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="legacyplugins"
	IntermediateDirectory="$(ConfigurationName)\legacy\$(ProjectName)"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories=".;&quot;$(SolutionDir)\..\..\legacy&quot;;&quot;$(SolutionDir)\..\..\include&quot;"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="legacyhost.lib"
		AdditionalLibraryDirectories="&quot;$(SolutionDir)\$(ConfigurationName)&quot;"
	/>
</VisualStudioPropertySheet>
